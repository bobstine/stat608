;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;                                                                ;;;;          Question:  Web browsers                               ;;;;                                                                ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;  (def nEach  40)  (def fileSize (sort-data                  (round (+ 20 (* 80 (uniform-rand nEach))))))  (def browser (repeat '(ms np) (list nEach nEach)))  (def x (mapcar #'(lambda (s) (if (eq s 'ms) 0 1)) browser))  (def y (+ (combine (repeat 11 nEach) (repeat 6 nEach))            (combine (/ fileSize 5) (/ fileSize 2.5))))    (def noise (combine (* 3.5 (normal-rand nEach))                        (* 6 (normal-rand nEach))))  (def p (plot-points (repeat fileSize 2) (+ y noise)))  (send p :point-symbol (iseq nEach) 'cross)  (send p :use-color t)  (send p :point-color  (iseq nEach) 'red)    (def x1 (repeat fileSize 2))  (regression-model (list x  x1 (* x1 x)) (+ noise y))  (write-data (transpose               (list (* .1 (round (* 10 (+ y noise))))                     browser (repeat fileSize 2)))               "Browser.dat" :path "Dave:")