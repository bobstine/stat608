;;;;;;;;          MBA Stat Example/Assignment Calculations;;;;(setf joint9 (list              (list .01 .02 .03 .04 .05)              (list .01 .03 .05 .07 .04)              (list .03 .06 .12 .06 .03)              (list .04 .07 .05 .03 .01)              (list .05 .04 .03 .02 .01)))(defun row-moments (joint values)  (let* ((marg (mapcar #'sum joint))         (mean (sum (* values marg)))         (x2   (sum (* values values marg)))         (var  (- x2 (* mean mean)))  )    (format t "Row Mean and Var is ~a  ~a  ~%" mean var)    (list mean var)    ))(defun col-moments (joint values)  (let* ((marg (mapcar #'sum (transpose joint)))         (mean (sum (* values marg)))         (x2   (sum (* values values marg)))         (var  (- x2 (* mean mean)))  )    (format t "Col Mean and Var is ~a  ~a  ~%" mean var)    (list mean var)    ))(defun covar (joint row col)  (let* ((rmv (row-moments joint row))         (cmv (col-moments joint col))         (exy (sum (* (combine (coerce (outer-product row col) 'list))                      (combine joint))))         (cov (- exy (* (first rmv) (first cmv))))  )    (format t "Covariance is ~a~%" cov)    (format t "Correlation is ~a~%"            (/ cov                (apply #'* (mapcar #'sqrt                                  (mapcar #'second (list rmv cmv))))))    ))          ; (covar joint9 (iseq 5) (iseq 5))#||(covar joint9 (iseq 5) (iseq 5))Row Mean and Var is 2  1.6  Col Mean and Var is 2  1.56  Covariance is -0.6Correlation is -0.379777||#;;;;  Example 9 of practice problems(row-moments joint9 (iseq 5))   ; 2   1.6(col-moments joint9 (iseq 5))   ; 2   1.56