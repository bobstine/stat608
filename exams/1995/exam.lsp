;;;;;;;;  Lisp code for handling grading;;;;;;  Set up files(def scoreFile "Dave:Text:Classes:Stat 608:Exams:Exam, 1995:Exam scores")(def choiceFile "Dave:Text:Classes:Stat 608:Exams:Exam, 1995:Exam choices")(def maxScore (* 4 42));;  Get the exam score data (seq #, ss #, points, scaled score)(def data (read-data-columns scoreFile))(format t "Number of students read: ~d~%" (setf n (length (first data))))(def ssn   (select data 1))(def score (* 100 (/ (select data 2) maxScore)))(def hp (histogram score))(format t "Summary of scores:~%  Mean = ~6,1f  SD = ~6,2f~%"        (mean score) (standard-deviation score))(format t "Fivenum = ~a~%" (fivnum score))(def np (plot-points (normal-cdf (/ (iseq 1 (length score)) (+ n 1)))                     (sort-data score)))